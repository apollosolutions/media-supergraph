{
  "name": "@apollosolutions/media-supergraph",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=dev nodemon index.js",
    "dev:router": "rover dev --supergraph-config supergraph-config-dev.yaml --router-config router-config-dev.yaml",
    "start": "node index.js",
    "subscription": "nodemon ./subgraphs/trending/subgraph.js",
    "test": "npm run test:compile",
    "test:compile": "tsc",
    "test:compose": "APOLLO_ELV2_LICENSE=accept rover supergraph compose --config supergraph-config-dev.yaml --output supergraph.graphql"
  },
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@apollo/subgraph": "^2.4.9",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.7.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.14.0",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "typescript": "^4.9.5"
  },
  "nodemonConfig": {
    "ext": "js,json,graphql"
  }
}
